{
  "name": "baruncorp-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prepare": "node -e \"try { require('husky').install() } catch (e) {if (e.code !== 'MODULE_NOT_FOUND') throw e}\"",
    "build": "nest build",
    "db:format": "pnpx prisma format",
    "db:generate": "pnpx prisma generate",
    "db:migrate": "pnpx prisma migrate dev",
    "db:push": "npx prisma db push",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "swagger": "pnpx swagger-typescript-api -p http://localhost:3000/api-json -o ./ -n api-spec.ts --axios --extract-request-params",
    "cqrs:generate": "node ./cqrs-generator/cli.mjs"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.9.1",
    "@nestjs/common": "^10.2.2",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.2.2",
    "@nestjs/cqrs": "^10.2.5",
    "@nestjs/event-emitter": "^2.0.2",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/platform-express": "^10.2.2",
    "@nestjs/swagger": "^7.1.10",
    "@prisma/client": "5.2.0",
    "@t00nday/nestjs-got": "^2.2.19",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "commander": "^11.0.0",
    "cookie-parser": "^1.4.6",
    "date-fns": "^2.30.0",
    "fs-extra": "^11.1.1",
    "got": "^11.0.0",
    "handlebars": "^4.7.8",
    "nodemailer": "^6.9.4",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.80",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.1",
    "@types/node": "18.16.12",
    "@types/nodemailer": "^6.4.10",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "commitlint-config-git-commit-emoji": "^1.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.3",
    "jest": "29.5.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.3.2",
    "prisma": "^4.15.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.1.0",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
